        }
      },
      "description": "GetUOMResponse is the response for getting a UOM."
    },
    "v1ImportError": {
      "type": "object",
      "properties": {
        "rowNumber": {
          "type": "integer",
          "format": "int32",
          "description": "Row number in the Excel file."
        },
        "field": {
          "type": "string",
          "description": "Field that caused the error."
        },
        "message": {
          "type": "string",
          "description": "Error message."
        }
      },
      "description": "ImportError represents a single import error."
    },
    "v1ImportUOMsRequest": {
      "type": "object",
      "properties": {
        "fileContent": {
          "type": "string",
          "format": "byte",
          "description": "Excel file content as bytes (.xlsx or .xls format)."
        },
        "fileName": {
          "type": "string",
          "description": "Original filename (for format detection).\nMust be a simple filename without path separators."
        },
        "duplicateAction": {
          "type": "string",
          "description": "Required. How to handle duplicates: \"skip\", \"update\", \"error\"."
        }
      },
      "description": "ImportUOMsRequest is the request for importing UOMs from Excel."
    },
    "v1ImportUOMsResponse": {
      "type": "object",
      "properties": {
        "base": {
          "$ref": "#/definitions/v1BaseResponse",
          "description": "Standard response metadata."
        },
        "successCount": {
          "type": "integer",
          "format": "int32",
          "description": "Number of successfully imported records."
        },
        "skippedCount": {
          "type": "integer",
          "format": "int32",
          "description": "Number of skipped records (duplicates with skip action)."
        },
        "updatedCount": {
          "type": "integer",
          "format": "int32",
          "description": "Number of updated records (duplicates with update action)."
        },
        "failedCount": {
          "type": "integer",
          "format": "int32",
          "description": "Number of failed records."
        },
        "errors": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/v1ImportError"
          },
          "description": "Details of failed records."
        }
      },
      "description": "ImportUOMsResponse is the response for importing UOMs."
    },
    "v1ListUOMsResponse": {
      "type": "object",
      "properties": {
        "base": {
          "$ref": "#/definitions/v1BaseResponse",
          "description": "Standard response metadata."
        },
        "data": {
          "type": "array",
          "items": {
            "type": "object",
