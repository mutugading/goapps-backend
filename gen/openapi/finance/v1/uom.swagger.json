        },
        "statusCode": {
          "type": "string",
          "description": "HTTP-like status code (e.g., \"200\", \"400\", \"404\", \"500\")."
        },
        "isSuccess": {
          "type": "boolean",
          "description": "Indicates if the operation was successful."
        },
        "message": {
          "type": "string",
          "description": "Human-readable message describing the result."
        }
      },
      "description": "BaseResponse is the standard response wrapper for all API responses."
    },
    "v1CreateUOMRequest": {
      "type": "object",
      "properties": {
        "uomCode": {
          "type": "string",
          "description": "Unique code (uppercase, alphanumeric with underscore, 1-20 chars).\nMust start with an uppercase letter. Immutable after creation."
        },
        "uomName": {
          "type": "string",
          "description": "Display name (1-100 chars)."
        },
        "uomCategory": {
          "$ref": "#/definitions/v1UOMCategory",
          "description": "Category (required, cannot be UNSPECIFIED)."
        },
        "description": {
          "type": "string",
          "description": "Optional description (max 500 chars)."
        }
      },
      "description": "CreateUOMRequest is the request for creating a new UOM."
    },
    "v1CreateUOMResponse": {
      "type": "object",
      "properties": {
        "base": {
          "$ref": "#/definitions/v1BaseResponse",
          "description": "Standard response metadata."
        },
        "data": {
          "$ref": "#/definitions/v1UOM",
          "description": "Created UOM data."
        }
      },
      "description": "CreateUOMResponse is the response for creating a UOM."
    },
    "v1DeleteUOMResponse": {
      "type": "object",
